#include <stdio.h>

int main() {
    int pos,i,n,value,ch,arr[100];
    printf("Enter the elements in array");
    scanf("%d",&n);
    arr[n];
    for(i=0;i<n;i++)
    {
        printf("Value for index %d :",i);
        scanf("%d",&arr[i]);
    }
    printf("Insert element at end \n");
    scanf("%d",&value);
    arr[n]=value;
    printf("Element %d is inserted at index \n",value,n);
    printf("New array is \n");
    for(i=0;i<n+1;i++)
    {
        printf("%d\t",arr[i]);
    }

    return 0;
}
OUTPUT:
Enter the elements in array
5
Value for index 0 :10
Value for index 1 :20
Value for index 2 :30
Value for index 3 :40
Value for index 4 :50
Insert element at end 
60
Element 60 is inserted at index 
New array is 
10	0	30	40	50	60	
